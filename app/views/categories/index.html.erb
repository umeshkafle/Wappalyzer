<h2>Categories List</h2>
<table>
	<% @categories.each do |category|%>
	<tbody>
		<div>
			<%= link_to "#{category.categoryName}", "http://#{category.categoryName.downcase}.com", :target => '_blank'%>
		</div>
		<div>
			<%= link_to 'Add Application', new_category_content_path(category)%>
			<%= link_to 'View Applications', category_contents_path(category)%>
		</div>
	</tbody>
</table>
<%end%>
<div>
	<%= link_to 'new Category', new_category_path%></div>